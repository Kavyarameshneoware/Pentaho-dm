        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>function convertToEpoch(timestamp) {
    var dateParts = timestamp.split(" ");
    var dateOnly = dateParts[0].split("/");  // Changed from "-" to "/"
    var timeOnly = dateParts[1].split(":");
    
    var year = parseInt(dateOnly[0], 10);
    var month = parseInt(dateOnly[1], 10) - 1;  // Months are 0-based in JS
    var day = parseInt(dateOnly[2], 10);
    
    var secondParts = timeOnly[2].split(".");  // Split to remove milliseconds
    var hour = parseInt(timeOnly[0], 10);
    var minute = parseInt(timeOnly[1], 10);
    var second = parseInt(secondParts[0], 10);  // Extract only the second part
    
    var date = new Date(year, month, day, hour, minute, second);
    return Math.floor(date.getTime() / 1000);
}

start_epoch = convertToEpoch(start_timestamp);
end_epoch = convertToEpoch(end_timestamp);
</jsScript_script>
