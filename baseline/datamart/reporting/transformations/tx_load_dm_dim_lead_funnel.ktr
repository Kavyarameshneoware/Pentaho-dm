    <connection>pgsql_pentaho_dm</connection>
    <sql>SELECT
    /* ---- WEB COLUMNS ---- */
    DISTINCT ON (w.unique_visitor_id)
    w.unique_visitor_id                AS "Id",
    w.action_time                      AS "Date",
    NULL::TEXT                         AS "Lead_source",
    w.utm_campaign                     AS "utm_Campaign",
    w.utm_content                      AS "utm_content",
    w.utm_source                       AS "utm_source",
    w.utm_term                         AS "utm_term",
    NULL::TEXT                         AS "Campaign",
    w.contact_email                    AS "Email",
    w.cookie_source_type               AS "Cookie Source Type",
    w.geo_ip_country                   AS "Country",
    '0. Website Visits'                AS "Funnel Stage",
    NULL::INTEGER                      AS "Act-On Primary Score",
    NULL::TEXT                         AS "ObjectType",
    NULL::TEXT                         AS "Name",
    NULL::TEXT                         AS "Opp Id",
    NULL::TEXT                         AS "Opportunity_URL__c",
    NULL::DOUBLE PRECISION             AS "Amount"
FROM pentaho_dm.smi.web_traffic_dim AS w
 
UNION ALL
 
SELECT
    /* ---- ACT-ON COLUMNS ---- */
    DISTINCT ON (a.lead_id)
    a.lead_id                          AS "Id",
    l.source_created_date              AS "Date",
    l.lead_source                      AS "Lead_source",
    w.utm_campaign                     AS "utm_Campaign",
    w.utm_content                      AS "utm_content",
    w.utm_source                       AS "utm_source",
    w.utm_term                         AS "utm_term",
    NULL::TEXT                         AS "Campaign",
    a.email                            AS "Email",
    w.cookie_source_type               AS "Cookie Source Type",
    a.business_country                 AS "Country",
    CASE
        WHEN a.acton_primary_score BETWEEN 1 AND 10  THEN '1. De Anon'
        WHEN a.acton_primary_score BETWEEN 11 AND 29 THEN '2. Engaged'
        WHEN a.acton_primary_score BETWEEN 30 AND 39 THEN '3. MAL'
        WHEN a.acton_primary_score BETWEEN 40 AND 60 THEN '4. MQL'
    END                                 AS "Funnel Stage",
    a.acton_primary_score               AS "Act-On Primary Score",
    a.object_type                       AS "ObjectType",
    NULL::TEXT                          AS "Name",
    NULL::TEXT                          AS "Opp Id",
    NULL::TEXT                          AS "Opportunity_URL__c",
    NULL::DOUBLE PRECISION              AS "Amount"
    
FROM pentaho_dm.smi.acton_contact_dim AS a
LEFT JOIN pentaho_dm.smi.lead_dim AS l
       ON a.lead_id = l.source_id
LEFT JOIN pentaho_dm.smi.web_traffic_dim AS w
       ON a.email = w.contact_email
 
UNION ALL
 
SELECT
    /* ---- SFDC COLUMNS ---- */
    CASE
        WHEN c.source_id IS NOT NULL THEN c.source_id
        ELSE ld.source_id
    END                                  AS "Id",
    o.source_created_date              AS "Date",
    ld.lead_source                     AS "Lead_source",
    NULL::TEXT                         AS "utm_Campaign",
    NULL::TEXT                         AS "utm_content",
    NULL::TEXT                         AS "utm_source",
    NULL::TEXT                         AS "utm_term",
    ca.name                            AS "Campaign",
    c.email                            AS "Email",
    NULL::TEXT                         AS "Cookie Source Type",
    c.mailing_country                  AS "Country",
    CASE
        WHEN o.stage_name IN ('1. Discovery Call', '2. SAL', '2.5 Nurture') THEN '5. SAL'
        WHEN o.stage_name IN ('3. Qualifying', '4. Validating', '5. Proposal', '6. Negotiating', '7. Finalizing') THEN '6. SQL'
        WHEN o.stage_name IN ('8. Opportunity Won Rep', '8.5 Opportunity Won Sales Operations') THEN '7. Opportunity Won'
        WHEN o.stage_name = '0. Opportunity Lost' THEN '0. Opportunity Lost'
    END                                 AS "Funnel Stage",
    NULL::INTEGER                      AS "Act-On Primary Score",
    NULL::TEXT                         AS "ObjectType",
    o.name                             AS "Name",
    o.source_id                        AS "Opp Id",
    CONCAT('https://pentaho.my.salesforce.com/', o.source_id) AS "Opportunity_URL__c",
    o.amount                           AS "Amount"
FROM pentaho_dm.smi.opportunity_dim AS o
LEFT JOIN pentaho_dm.smi.lead_dim AS ld
       ON o.source_id = ld.converted_opportunity_id
LEFT JOIN pentaho_dm.smi.contact_dim AS c
       ON o.contact_id = c.source_id
LEFT JOIN pentaho_dm.smi.campaign_dim AS ca
       ON o.campaign_id = ca.source_id;</sql>
